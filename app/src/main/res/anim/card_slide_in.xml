<?xml version="1.0" encoding="utf-8"?>
<set xmlns:android="http://schemas.android.com/apk/res/android"
	android:interpolator="@android:interpolator/decelerate_quad">
	<translate android:fromYDelta="100%p" android:toYDelta="0"
	           android:fromXDelta="50%p" android:toXDelta="0"
	           android:duration="@android:integer/config_mediumAnimTime"/>
	<alpha android:fromAlpha="0.6" android:toAlpha="1.0"
	       android:duration="@android:integer/config_mediumAnimTime" />
	<rotate android:fromDegrees="45.0" android:toDegrees="0.0"
		android:duration="@android:integer/config_shortAnimTime"/>
	<scale android:fromXScale="0.7" android:toXScale="1.0"
	       android:fromYScale="0.55" android:toYScale="1.0"
	       android:duration="@android:integer/config_mediumAnimTime" />
</set>
